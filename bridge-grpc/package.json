{
  "name": "eywa-solana-bridge-grpc",
  "version": "0.0.2",
  "scripts": {
    "docker:build": "cd .. && docker build -f .k8s/docker/Dockerfile-bridge-grpc -t eywa-solana-bridge-grpc . && cd bridge-grpc",
    "docker:stop": "docker stop eywa-solana-bridge-grpc",
    "docker:logs": "docker logs eywa-solana-bridge-grpc",
    "docker:start": "docker run --rm --name eywa-solana-bridge-grpc -t -p 127.0.0.1:8080:8080 -p 127.0.0.1:8081:8081 eywa-solana-bridge-grpc:latest",
    "test1": "ts-node ./tests/subscribe-oracle-request.ts",
    "test": "ts-node ./tests/subscribe-slot-change.ts",
    "gen": "grpc_tools_node_protoc -I ./proto --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./proto --js_out=import_style=commonjs,binary:./proto --grpc_out=grpc_js:./proto  ./proto/eywa_solana.proto",
    "gen:help": "grpc_tools_node_protoc --help",
    "start": "ts-node server.ts",
    "start:client": "ts-node test-client.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.3.7",
    "@grpc/proto-loader": "^0.6.5",
    "google-protobuf": "^3.18.1",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.11.2"
  }
}
